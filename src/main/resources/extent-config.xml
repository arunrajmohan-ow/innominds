<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <!-- Report Configuration -->
    <configuration>
        <!-- Theme can be standard or dark -->
        <theme>standard</theme>
        <!-- Encoding for the report -->
        <encoding>UTF-8</encoding>
        <!-- Timestamp format for the report -->
        <protocol>Standard</protocol>
        <timestamp format="yyyy-MM-dd HH:mm:ss">true</timestamp>
    </configuration>
    <!-- Build information -->
    <build>
        <version>1.0.0</version>
        <release-date>2023-01-01</release-date>
    </build>
    <!-- System information -->
    <system-information>
        <parameter>
            <name>Environment</name>
            <value>Production</value>
        </parameter>
    </system-information>
</extentreports>